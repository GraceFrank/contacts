{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "target": "es6",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "./dist",
      "baseUrl": ".",
      "paths": {
          "*": [
              "node_modules/*",
              "src/types/*"
          ]
      },
  },
  "files": ["src/app/models/types.ts"],
  "include": [
      "src/**/*"
  ],
    "exclude": ["src/types/*.ts", "node_modules", ".vscode", ".idea"]

} 